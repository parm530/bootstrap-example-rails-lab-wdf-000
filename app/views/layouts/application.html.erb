<!DOCTYPE html>
<html>
<head>
  <title>Application Layout</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
</head>
<body>

<div class="container">
  <ul class="nav navbar-nav">
    <li><a href="#">Home</a></li>
    <li><a href="#">About</a></li>
    <li><a href="#">Contact</a></li>  
  </ul>

  <div class="row">
    <div class="col-lg-4">
      <p>Row #1 Column #1</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
    <div class="col-lg-4">
      <p>Row #1 Column #2</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
    <div class="col-lg-4">
      <p>Row #1 Column #3</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
  </div>

  <div class="row" >
    <div class="col-lg-4">
      <p>Row #2 Column #1</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
        <div class="col-lg-4">
      <p>Row #2 Column #2</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
    <div class="col-lg-4">
      <p>Row #2 Column #3</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-4">
      <p>Row #3 Column #1</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
        <div class="col-lg-4">
      <p>Row #3 Column #2</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
    <div class="col-lg-4">
      <p>Row #3 Column #3</p>
      <button class="btn btn-default" type="submit">Button</button>
    </div>
  </div>
  
</div>

<%= yield %>
<%= javascript_include_tag "application" %>
</body>
</html>
